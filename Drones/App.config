<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="akka" type="Akka.Configuration.Hocon.AkkaConfigurationSection, Akka" />
	</configSections>

	<akka>
		<hocon>
			<![CDATA[
				akka {
					actor {
						provider = remote
					}
					
					remote {
						dot-netty.tcp {
							port = 8081
							hostname = localhost
						}
					}
				}
			]]>
		</hocon>
	</akka>
</configuration>